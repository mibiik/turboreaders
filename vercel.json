{
  "buildCommand": "cd fastre && chmod +x build.sh && ./build.sh",
  "outputDirectory": "fastre/.next",
  "devCommand": "cd fastre && npm run dev",
  "installCommand": "cd fastre && npm install --legacy-peer-deps",
  "framework": "nextjs",
  "rewrites": [
    { "source": "/(.*)", "destination": "/fastre/$1" }
  ],
  "git": {
    "submodules": false,
    "deploymentEnabled": {
      "main": true
    },
    "cloneIgnorePatterns": ["fastre/.git", "node_modules"]
  }
} 